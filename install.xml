    <search><![CDATA[<li><a href="{{ wishlist }}">{{ text_wishlist }}</a></li>]]></search>
    <add position="after">
    <![CDATA[<li><a href="index.php?route=extension/module/cubit/account">Membership</a></li>]]>
    </add>
    </operation>
    </file>
</modification>

